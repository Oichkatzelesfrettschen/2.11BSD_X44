# $211BSD:
#
# Evdev Protocol Support 

defflag opt_evdev.h EVDEV_SUPPORT

device  evdev
attach	evdev at wsmousedev with evdev_mouse
attach	evdev at wskbddev with evdev_kbd

file 	dev/misc/evdev/evdev_mouse.c	evdev		       needs-flag		
file 	dev/misc/evdev/evdev_kbd.c		evdev			   needs-flag
file	dev/misc/evdev/evdev.c			evdev	           #needs-flag
file	dev/misc/evdev/evdev_mt.c		evdev
file	dev/misc/evdev/evdev_support.c	evdev
file	dev/misc/evdev/evdev_utils.c	evdev
