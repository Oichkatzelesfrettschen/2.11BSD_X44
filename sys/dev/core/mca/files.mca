# $NetBSD: files.mca,v 1.18 2002/03/24 18:03:58 jdolecek Exp $
#
# Config.new file and device description for machine-independent MCA code.
# Included by ports that need it.

defflag	MCAVERBOSE

device	mca {[slot = -1]}
attach	mca at mcabus
file	dev/core/mca/mca.c			mca			needs-flag
file	dev/core/mca/mca_subr.c		mca

# serial port devices
attach	com at mca with com_mca
file	dev/core/mca/com_mca.c		com_mca
